<?import javafx.scene.shape.ClosePath?>
<?import javafx.scene.shape.LineTo?>
<?import javafx.scene.shape.MoveTo?>
<?import javafx.scene.shape.Path?>

<!-- plus icon for nodes whose children are hidden -->
<Path fx:id="plus" stroke="white" fill="gray" strokeWidth="2"
      xmlns:fx="http://javafx.com/fxml">
  <elements>
    <MoveTo x="5" y="0"/>
    <LineTo x="13" y="0"/>
    <LineTo x="13" y="5"/>
    <LineTo x="18" y="5"/>
    <LineTo x="18" y="13"/>
    <LineTo x="13" y="13"/>
    <LineTo x="13" y="18"/>
    <LineTo x="5" y="18"/>
    <LineTo x="5" y="13"/>
    <LineTo x="0" y="13"/>
    <LineTo x="0" y="5"/>
    <LineTo x="5" y="5"/>
    <ClosePath/>
  </elements>
</Path>